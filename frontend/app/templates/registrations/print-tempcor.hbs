<table class="table borderless table-condensed print-assessment">
	<tbody>
		<tr>
			<td width="15%">Student Name</td>
			<td>: <strong>{{meta.studfullname}}</strong></td>
			<td width="15%">School Year</td>
			<td>: <strong>{{meta.sy}}</strong></td>
		</tr>
		<tr>
			<td width="15%">Student ID</td>
			<td>: <strong>{{meta.studid}}</strong></td>
			<td width="15%">Semester</td>
			<td>: <strong>{{meta.sem}}</strong></td>
		</tr>
		<tr>
			<td width="15%">Course &amp; Year</td>
			<td>: <strong>{{meta.studmajor}} - {{meta.studlevel}}</strong></td>
			<td width="15%">Load Units</td>
			<td>: <strong>{{meta.units}}</strong></td>
		</tr>
	</tbody>
</table>
<hr class="printhr">
<table class="table table-striped table-condensed print-assessment">
	<thead>
		<tr>
			<th>SUBJECT CODE</th>
			<th>SECTION</th>
			<th width="40%">SUBJECT DESCRIPTION</th>
			<th class="text-right" width="7%">DAYS</th>
			<th class="text-right" width="7%">TIME</th>
			<th class="text-right" width="7%">BLDG</th>
			<th class="text-right" width="7%">ROOM</th>
			<th class="text-right" width="7%">UNIT</th>
		</tr>
	</thead>
	<tbody>
		{{#each subj}}
		<tr>
			<td>{{subjcode}}</td>
			<td>{{section}}</td>
			<td>{{subjdesc}}</td>
			<td class="text-right" >&nbsp;</td>
			<td class="text-right" >&nbsp;</td>
			<td class="text-right" >&nbsp;</td>
			<td class="text-right" >&nbsp;</td>
			<td class="text-right" >{{subjcredit}}</td>
		</tr>
		{{/each}}
	</tbody>
</table>
<div class="text-center">
	<span><em>***NOTHING FOLLOWS***</em></span>
</div>
<br>
<br>
<br>
<table class="table table-sign">
	<thead>
		<th width="16%" class="text-center signline">DEAN</th>
		<th width="5%">&nbsp;</th>
		<th width="16%" class="text-center signline">MEDICAL</th>
		<th width="5%">&nbsp;</th>
		<th width="16%" class="text-center signline">DENTAL</th>
		<th width="5%">&nbsp;</th>
		<th width="16%" class="text-center signline">PTGEA</th>
		<th width="5%">&nbsp;</th>
		<th width="16%" class="text-center signline">CASHIER</th>
	</thead>
</table>
